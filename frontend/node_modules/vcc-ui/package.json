{
	"name": "vcc-ui",
	"version": "1.0.4",
	"license": "LicenseRef-LICENSE",
	"main": "lib/index.js",
	"module": "es/index.js",
	"jsnext:main": "es/index.js",
	"sideEffects": false,
	"files": [
		"README.md",
		"LICENSE",
		"assets/**",
		"lib/**",
		"es/**",
		"_src/**"
	],
	"scripts": {
		"build:src": "rsync -avm --include='/*' --exclude='__snapshots__/*.*' --exclude='stories.js' --exclude='test.js' --exclude='test/**' ./src/ ./_src/",
		"build:es": "rm -rf es && babel src -d es",
		"build:lib": "rm -rf lib && BABEL_ENV=commonjs babel src -d lib",
		"build": "npm run build:es && npm run build:lib && npm run build:src",
		"check": "npm run format && npm run lint && npm run test",
		"prepare": "npm run-script build",
		"storybook": "start-storybook -p 9001 -c .storybook -s ./public",
		"storybook-build": "./node_modules/.bin/build-storybook -s ./public -c .storybook -o out --quiet",
		"build-site": "./bin/build-site.sh",
		"test": "jest --config=jest.config.js",
		"lint": "eslint src --ext .js",
		"serve": "serve out -p 3000",
		"format": "prettier --write \"src/**/*.js\""
	},
	"dependencies": {
		"fela": "^10.4.1",
		"fela-plugin-rtl": "^10.4.1",
		"fela-preset-web": "^10.4.1",
		"fela-sort-classnames": "^10.4.1",
		"hex-to-rgba": "^1.0.2",
		"react-fela": "^10.4.1"
	},
	"peerDependencies": {
		"react": "^16.8.0",
		"react-dom": "^16.8.0",
		"react-transition-group": "^2.6.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@babel/plugin-proposal-class-properties": "^7.2.1",
		"@babel/plugin-transform-modules-commonjs": "^7.4.4",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-react": "^7.0.0",
		"@storybook/addon-actions": "^5.1.7",
		"@storybook/addon-backgrounds": "^5.1.7",
		"@storybook/addon-knobs": "^5.1.7",
		"@storybook/addon-viewport": "^5.1.7",
		"@storybook/cli": "^5.1.7",
		"@storybook/react": "^5.1.7",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^9.0.0",
		"babel-jest": "^24.8.0",
		"babel-loader": "^8.0.0-beta.6",
		"babel-plugin-require-context-hook": "^1.0.0",
		"core-js": "^3.1.3",
		"cross-env": "5.0.5",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-compat": "^2.6.3",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-react": "^7.10.0",
		"fela-monolithic": "^10.1.3",
		"jest": "^24.8.0",
		"jest-react-fela": "^10.1.3",
		"jsdom": "^11.12.0",
		"prettier": "1.14.0",
		"prop-types": "^15.6.2",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-test-renderer": "^16.8.3",
		"serve": "^10.0.0",
		"storybook-addon-jsx": "^7.1.2"
	},
	"browserslist": [
		"IE >= 11",
		"Firefox >= 60",
		"Safari >= 11.1",
		"Chrome >= 66",
		"ChromeAndroid >= 66",
		"iOS >= 11.3",
		"Edge >= 15"
	]
}
